<h2>Ajouter un professeur</h2>

<% include navbar %>

<p>Selection d'un établissement pour le moment (à supprimer plus tard)</p>
<select name="etablissement" required>
    <% for(var i=0;i< etablissements.length; i++){%>
    <option value="<%= etablissements[i]._id%>"><%= etablissements[i].nom%></option>
    <%}%>
</select>
<hr/>
<select name="sexe" required>
    <option value="Mr">Mr</option>
    <option value="Mme">Mme-</option>
</select>
<label for="professeur">Nom: </label>
<input type="text" name="professeur" id="professeur" required>
<input type="submit" value="Inscrire">